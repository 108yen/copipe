    "@yamada-ui/react": "^1.7.4",
    "axios": "^1.6.5",
    "dayjs": "^1.11.10",
    "jotai": "^2.6.1",
    "next": "15.2.0",
    "next-auth": "^4.24.5",
    "react": "19.0.0",
