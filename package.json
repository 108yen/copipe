{
  "name": "copipe",
  "version": "1.8.1",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "esbuild",
      "prisma",
      "sharp",
      "@prisma/client",
      "lefthook"
    ]
  },
  "scripts": {
    "prepare": "lefthook install",
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint . --max-warnings=0 --cache",
    "lint:inspect": "pnpm dlx @eslint/config-inspector",
    "format": "prettier --write .",
    "format:check": "prettier --check --cache .",
    "format:write": "prettier --write --cache .",
    "postinstall": "prisma generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "typecheck": "tsc --noEmit",
    "clean": "pnpx rimraf .turbo .next node_modules pnpm-lock.yaml"
  },
  "dependencies": {
    "@next/third-parties": "16.1.4",
    "@prisma/client": "6.19.2",
    "@supabase/supabase-js": "^2.39.2",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0",
    "@yamada-ui/lucide": "^1.10.5",
    "@yamada-ui/react": "^1.7.4",
    "axios": "^1.6.5",
    "dayjs": "^1.11.10",
    "jotai": "^2.6.1",
    "next": "16.1.5",
    "next-auth": "^4.24.5",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.49.2",
    "typescript": "^5.6.3",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@cspell/eslint-plugin": "^9.0.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.25.1",
    "@next/eslint-plugin-next": "^16.0.0",
    "@types/gtag.js": "^0.0.20",
    "@types/node": "^24.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@typescript-eslint/utils": "^8.33.1",
    "eslint": "^9.39.0",
    "eslint-config-next": "16.1.4",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^5.0.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.0.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "eslint-typegen": "^2.2.0",
    "globals": "^16.0.0",
    "lefthook": "^2.0.0",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.1.1",
    "prisma": "6.19.2",
    "typescript-eslint": "^8.21.0"
  }
}
