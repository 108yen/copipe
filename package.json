    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.1.1",
    "storybook": "^7.6.10",
    "storybook-dark-mode": "^3.0.3"
